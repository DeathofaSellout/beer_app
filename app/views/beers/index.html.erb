
<nav>
  <div class="nav-wrapper">
    <a href="#" class="brand-logo">Logo</a>
    <ul id="nav-mobile" class="right hide-on-med-and-down">
      <li><%= link_to "Log out", logout_path, class: "btn-flat" %></li>

    </ul>
  </div>
</nav>

<div class="col s12 m7">
  <% @beers.each do |b| %>
    <div class="card horizontal">
      <div class="card-image beer_image">
        <%= image_tag(b.image, :class => "beer_image") %>
      </div>
      <div class="card-stacked">
        <div class="card-content">
          <ul>
            <li><%= b.name%></li>
            <li><%= b.style%></li>
            <li><%= b.brewery%></li>
            <li><%= b.country%></li>
            <li><%= b.ibu%></li>
            <li><%= b.abv%></li>
          </ul>
        </div>
        <div class="card-action">
          <a href="#">This is a link</a>
        </div>
      </div>
    </div>
  <% end %>
</div>
